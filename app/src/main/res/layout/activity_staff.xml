<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ActivityStaff">

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/botNavViewPersonelActivity"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="#FAFAC9"
        android:padding="10dp"
        app:itemIconSize="30dp"
        app:itemIconTint="@color/red"
        app:itemTextColor="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:menu="@menu/staff_nav_menu" />

    <FrameLayout
        android:id="@+id/frameLayoutPersonelActivity"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/background"
        app:layout_constraintBottom_toTopOf="@+id/botNavViewPersonelActivity"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    </FrameLayout>
</androidx.constraintlayout.widget.ConstraintLayout>